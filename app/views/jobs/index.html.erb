<p style="color: green"><%= notice %></p>

<h1>Jobs</h1>

<%= include_gon %>

<% provide :head_tags do %>
  <%= javascript_import_module_tag "map" %>
<% end %>
<div id="map" data: {@scrape.to_json}></div>

<div id="jobs">
  <% @jobs.each do |job| %>
    <%= render job %>
    <p>
      <%= link_to "Show this job", job %>
    </p>
  <% end %>
</div>

<%#= turbo_frame_tag "modal" %>

