<p style="color: green"><%= notice %></p>

<%= include_gon %>
<div class="index">
  <h2><%=@jobs.length%> Jobs Saved</h2>

  <% provide :head_tags do %>
    <%= javascript_import_module_tag "map" %>
  <% end %>
  <div id="map" data: {@scrape.to_json}></div>

  <div id="jobs">
    <% @jobs.each do |job| %>
      <%= render job %>
    <% end %>
  </div>
</div>

